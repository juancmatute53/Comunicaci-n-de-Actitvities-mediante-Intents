<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MenuActivity">
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="355dp"
            android:layout_height="59dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout10"
            app:layout_constraintVertical_bias="0.2">
        <TextView
                android:text="Instrumentos Musicales"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView3" android:textSize="24sp"
                android:textAlignment="center" android:textStyle="bold"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="357dp"
            android:layout_height="51dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintVertical_bias="0.975" android:id="@+id/linearLayout9">
        <Button
                android:text="Enviar Comentario"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/btnComentario" android:onClick="irComentario"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="344dp"
            android:layout_height="480dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout9" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.759" app:layout_constraintHorizontal_bias="0.492"
            android:id="@+id/linearLayout10">
        <ImageButton
                android:layout_width="match_parent"
                android:layout_height="wrap_content" app:srcCompat="@drawable/guir12" android:id="@+id/imageButton9"
                android:onClick="irGuitarras"/>
        <ImageButton
                android:layout_width="match_parent"
                android:layout_height="wrap_content" app:srcCompat="@drawable/pia123" android:id="@+id/imageButton10"/>
        <ImageButton
                android:layout_width="match_parent"
                android:layout_height="wrap_content" app:srcCompat="@drawable/bate1" android:id="@+id/imageButton11"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>